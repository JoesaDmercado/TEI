<?xml-model href="http://tei.it.ox.ac.uk/jenkins/job/TEIP5/lastSuccessfulBuild/artifact/release/xml/tei/odd/p5.nvdl" type="application/xml" schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="header" ident="xenoData">
  <gloss versionDate="2015-05-30" xml:lang="en">outside metadata</gloss>
  <desc versionDate="2015-05-30" xml:lang="en">provides a container element into which metadata elements from non-TEI languages may be placed.</desc>
  <classes>
    <memberOf key="att.global"/>
    <memberOf key="model.teiHeaderPart"/>
  </classes>
  <content>
    <group xmlns="http://relaxng.org/ns/structure/1.0">
      <ref name="macro.anyXML"/>
    </group>
  </content>
  <exemplum versionDate="2015-05-30"
	    xml:lang="en"
	    xmlns:cc="http://web.resource.org/cc/"
	    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	    xmlns:dc="http://purl.org/dc/elements/1.1/">
    <egXML valid="false" xmlns="http://www.tei-c.org/ns/Examples"> <xenoData>
	<rdf:RDF>
	  <cc:Work rdf:about="">
	    <dc:title>Applied Software Project Management  - review</dc:title>
	    <dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"></dc:type>
	    <dc:license rdf:resource="http://creativecommons.org/licenses/by-sa/2.0/uk/"/>
	  </cc:Work>
	  <cc:License rdf:about="http://creativecommons.org/licenses/by-sa/2.0/uk/">
	    <cc:permits rdf:resource="http://web.resource.org/cc/Reproduction"/>
	    <cc:permits rdf:resource="http://web.resource.org/cc/Distribution"/>
	    <cc:requires rdf:resource="http://web.resource.org/cc/Notice"/>
	    <cc:requires rdf:resource="http://web.resource.org/cc/Attribution"/>
	    <cc:permits rdf:resource="http://web.resource.org/cc/DerivativeWorks"/>
	    <cc:requires rdf:resource="http://web.resource.org/cc/ShareAlike"/>
	  </cc:License>
	</rdf:RDF>
      </xenoData></egXML>
  </exemplum>
  <exemplum versionDate="2015-05-30" xml:lang="en"
	   xmlns:dc="http://purl.org/dc/elements/1.1/">
    <egXML xmlns="http://www.tei-c.org/ns/Examples"
	   valid="false">
      <xenoData>
	<dc:title>British Library Harley MS 4955</dc:title>
	<dc:creator>William Cavendish, Earl (later Duke) of Newcastle</dc:creator>
	<dc:creator>John Rolleston, secretart to William Cavendish</dc:creator>
	<dc:rights>Not to be shared or published without permission</dc:rights>
	<dc:source>British Library Harley 4955 The Newcastle MS</dc:source>
      </xenoData>
    </egXML>
  </exemplum>
  <remarks versionDate="2010-04-30" xml:lang="en">
    <p>The metadata stored in <gi>xenoData</gi> may be derived from
    the <gi>teiHeader</gi> (and then tucked back in it), or copied
    from other sources, e.g. those from wich other portions of the
    <gi>teiHeader</gi> are derived.</p>
  </remarks>
  <listRef>
    <ptr target="#HD9"/>
  </listRef>
</elementSpec>
